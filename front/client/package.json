{
  "name": "client",
  "version": "1.0.0",
  "description": "This is the frontend client for our dapp. It is built with Next.js and uses a render-prop pattern (via `lib/Web3Container.js`) so we can easily inject blockchain functionality (i.e. web3, accounts, and the contract instance) into each page.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next dev -p 3000",
    "debug": "node --inspect-brk -p 3000 ./node_modules/next/dist/bin/next",
    "build": "next build",
    "start": "next start -p 3500",
    "lint": "next lint",
    "link-contracts": "cd lib && ln -s ../../build/contracts contracts"
  },
